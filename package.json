{
  "name": "kon",
  "version": "0.1.0",
  "description": "Themis wrapper for Wafter",
  "main": "src/server.js",
  "scripts": {
    "build": "babel src/ -d build/",
    "start": "npm run build && node build/server.js",
    "dev": "nodemon --exec babel-node src/server.js",
    "clean": "rmdir /s /q build/",
    "pkg:win": "set NODE_ENV=production && npm run build && pkg -t node10-win -o dist/kon build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnma0/kon.git"
  },
  "author": "vnma0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnma0/kon/issues"
  },
  "homepage": "https://github.com/vnma0/kon#readme",
  "dependencies": {
    "dotenv": "^6.2.0",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.16.4",
    "extract-zip": "^1.6.7",
    "helmet": "^3.15.0",
    "isbinaryfile": "^4.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^5.11.1",
    "nodemon": "^1.18.9"
  }
}
